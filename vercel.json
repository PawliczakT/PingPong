{
  "buildCommand": "npx expo prebuild --platform web --no-install && npx expo export -p web",
  "outputDirectory": "dist",
  "devCommand": "expo",
  "cleanUrls": true,
  "framework": null,
  "rewrites": [
    {
      "source": "/api/trpc/(.*)",
      "destination": "/backend/api/trpc/[trpc]"
    },
    {
      "source": "/:path*",
      "destination": "/"
    }
  ]
}

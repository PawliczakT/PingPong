{
  "name": "expo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --tunnel",
    "start-web": "expo start --web --tunnel",
    "web": "expo start --web",
    "start-web-dev": "DEBUG=expo* expo start --web --tunnel",
    "build": "expo export -p web",
    "build:web": "expo export -p web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e:build": "detox build",
    "e2e:test": "detox test",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": false,
        "exclude": [],
        "listUnknownPackages": true
      }
    }
